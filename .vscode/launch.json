{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (Select Scene)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "requireExactSource": false,
            "program": "F:/godotC#/Godot_v4.2.1-stable_mono_win64/Godot_v4.2.1-stable_mono_win64.exe",
            "args": [
                "--fexec-charset=uff8",
                "--path",
                "${workspaceRoot}",
                "${command:godot.csharp.getLaunchScene}"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "console": "internalConsole",
        },
        {
            "name": "C# Godot",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "F:/godotC#/Godot_v4.2.1-stable_mono_win64/Godot_v4.2.1-stable_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "requireExactSource": false,
            "stopAtEntry": false,
            "args": [
                "--path",
                "${workspaceRoot}"
            ]
        },
        {
            "name": "Launch in Editor",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "F:/godotC#/Godot_v4.2.1-stable_mono_win64/Godot_v4.2.1-stable_mono_win64.exe",
            // "program": "env:GODOT4",
            "args": [
                "--path",
                "${workspaceRoot}",
                "--editor"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
        },
        {
            "name": "GDScript",
            "type": "godot",
            "request": "launch",
            "project": "${workspaceFolder}",
            "address": "127.0.0.1",
            "port": 6007,
            "scene": "pinned",
            "editor_path": "F:/godotC#/Godot_v4.2.1-stable_mono_win64/Godot_v4.2.1-stable_mono_win64.exe",
            // engine command line flags
            "profiling": false,
            "single_threaded_scene": false,
            "debug_collisions": false,
            "debug_paths": false,
            "debug_navigation": false,
            "debug_avoidance": false,
            "debug_stringnames": false,
            "frame_delay": 0,
            "time_scale": 1.0,
            "disable_vsync": false,
            "fixed_fps": 60,
            // anything else
            "additional_options": ""
        }
    ]
}